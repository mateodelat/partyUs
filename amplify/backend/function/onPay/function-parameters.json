{
  "permissions": {
    "api": {
      "partyUsAPI": ["Query", "Mutation"]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "partyusgraphqlOperations",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true,
      "env": "production"
    },
    {
      "type": "ProjectLayer",
      "resourceName": "partyuslayer",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true,
      "env": "production"
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "secretKeyProd",
      "environmentVariableName": "SECRET_KEY_PROD"
    },
    {
      "cloudFormationParameterName": "secretKeyStag",
      "environmentVariableName": "SECRET_KEY_STAG"
    },

    {
      "cloudFormationParameterName": "merchantIdProd",
      "environmentVariableName": "MERCHANT_ID_PROD"
    },
    {
      "cloudFormationParameterName": "merchantIdStag",
      "environmentVariableName": "MERCHANT_ID_STAG"
    }
  ]
}
