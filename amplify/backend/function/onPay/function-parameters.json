{
  "permissions": {
    "api": {
      "partyUsAPI": [
        "Query",
        "Mutation"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "partyusgraphqlOperations",
      "env": "staging",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    },
    {
      "type": "ProjectLayer",
      "resourceName": "partyuslayer",
      "env": "staging",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "merchantId",
      "environmentVariableName": "MERCHANT_ID"
    },
    {
      "cloudFormationParameterName": "secretKey",
      "environmentVariableName": "SECRET_KEY"
    }
  ]
}