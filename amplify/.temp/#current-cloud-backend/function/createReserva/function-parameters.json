{
  "permissions": {
    "api": {
      "partyUsAPI": [
        "Query",
        "Mutation"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "partyuscapaPostSignUp",
      "env": "staging",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "cognitoAppClientWeb",
      "environmentVariableName": "COGNITO_APP_CLIENT_WEB"
    },
    {
      "cloudFormationParameterName": "adminPassword",
      "environmentVariableName": "ADMIN_PASSWORD"
    },
    {
      "cloudFormationParameterName": "adminUsername",
      "environmentVariableName": "ADMIN_USERNAME"
    }
  ]
}